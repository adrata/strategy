{
  "name": "adrata",
  "version": "1.0.2",
  "private": true,
  "workspaces": [
    "mobile"
  ],
  "scripts": {
    "postinstall": "prisma generate --schema=prisma/schema-streamlined.prisma",
    "prebuild": "echo 'Starting build at' && echo %date% %time%",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "run:buyer-group": "node dist-scripts/scripts/run-buyer-group.js --company=\"Dell Technologies\" --profile=buyer-group-intelligence --limit=100",
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev -p 3000",
    "dev:fast": "cross-env NODE_OPTIONS=--max-old-space-size=2048 next dev -p 3000 --turbo",
    "build": "prisma generate --schema=prisma/schema-streamlined.prisma && next build && node scripts/fix-vercel-build.js",
    "build:fast": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "lint": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next lint",
    "type-check": "cross-env NODE_OPTIONS=--max-old-space-size=6144 npx tsc --noEmit --skipLibCheck",
    "type-check:fast": "cross-env NODE_OPTIONS=--max-old-space-size=4096 npx tsc --noEmit --skipLibCheck --incremental",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cleanup:deletion": "tsx src/platform/jobs/cleanup-job.ts",
    "cleanup:deletion:dry-run": "tsx src/platform/jobs/cleanup-job.ts --dry-run",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration/",
    "test:e2e": "playwright test",
    "setup:gmail-calendar": "./scripts/setup-gmail-calendar-nango.sh",
    "verify:nango": "node scripts/verify-gmail-calendar-nango-config.js",
    "vercel:env:gmail-calendar": "./scripts/generate-vercel-env-commands.sh",
    "test:ai-context": "jest tests/unit/ai-context/ tests/integration/ai-context/ && playwright test tests/e2e/ai-context/",
    "test:ai-context:coverage": "jest tests/unit/ai-context/ tests/integration/ai-context/ --coverage",
    "test:ai-context:unit": "jest tests/unit/ai-context/",
    "test:ai-context:integration": "jest tests/integration/ai-context/",
    "test:ai-context:e2e": "playwright test tests/e2e/ai-context/",
    "chronicle:daily": "node scripts/chronicle-scheduler.js DAILY",
    "chronicle:weekly": "node scripts/chronicle-scheduler.js WEEKLY",
    "chronicle:biweekly": "node scripts/chronicle-scheduler.js BIWEEKLY",
    "chronicle:status": "node scripts/chronicle-scheduler.js STATUS",
    "chronicle:test": "node scripts/test-chronicle-integration.js",
    "test:state-ranking": "node scripts/test-state-ranking.js",
    "test:state-ranking:unit": "jest tests/unit/ranking/",
    "test:state-ranking:integration": "jest tests/integration/api/state-ranking-api.test.ts",
    "test:state-ranking:e2e": "playwright test tests/e2e/state-based-ranking.spec.ts",
    "test:state-ranking:coverage": "jest tests/unit/ranking/ --coverage",
    "test:intelligence-fixes": "tsx scripts/test-intelligence-summary-fixes.ts",
    "fix:intelligence-classifications": "tsx scripts/fix-incorrect-intelligence-classifications.ts",
    "fix:intelligence-classifications:dry-run": "tsx scripts/fix-incorrect-intelligence-classifications.ts --dry-run",
    "pre-generate:intelligence": "tsx scripts/pre-generate-intelligence-for-fixed-companies.ts",
    "audit:intelligence-regeneration": "tsx scripts/audit-companies-needing-regeneration.ts",
    "regenerate:intelligence": "tsx scripts/regenerate-intelligence-for-audited-companies.ts",
    "verify:intelligence": "tsx scripts/verify-intelligence-accuracy.ts",
    "test:e2e:auth": "playwright test tests/e2e/auth.spec.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ci": "playwright test --reporter=github",
    "test:auth": "bash scripts/run-auth-tests.sh",
    "test:auth:headed": "bash scripts/run-auth-tests.sh headed",
    "test:auth:ui": "bash scripts/run-auth-tests.sh ui",
    "test:auth:debug": "bash scripts/run-auth-tests.sh debug",
    "desktop:dev": "cross-env NEXT_PUBLIC_IS_DESKTOP=true tauri dev",
    "desktop:build": "node scripts/cross-platform-build.js",
    "desktop:build:legacy": "npm run clean:full && npm run tauri:prep && node scripts/prepare-desktop-build.js && cross-env NODE_ENV=production NEXT_PUBLIC_IS_DESKTOP=true NEXT_PUBLIC_USE_STATIC_EXPORT=true TAURI_BUILD=true npm run build && npm run tauri:fix-paths && node scripts/restore-desktop-build.js && cross-env TAURI_BUILD=true tauri build && npm run tauri:restore && npm run postdesktop:build",
    "desktop:build:demo": "npm run clean:full && npm run tauri:prep && node scripts/prepare-desktop-build.js && cross-env NODE_ENV=production NEXT_PUBLIC_IS_DESKTOP=true NEXT_PUBLIC_USE_STATIC_EXPORT=true TAURI_BUILD=true npm run build && npm run tauri:fix-paths && node scripts/restore-desktop-build.js && tauri build --config src-desktop/tauri.conf.demo.json && npm run tauri:restore && npm run postdesktop:build:demo",
    "desktop:build:windows": "npm run clean:full && npm run tauri:prep && node scripts/prepare-desktop-build.js && cross-env NODE_ENV=production NEXT_PUBLIC_IS_DESKTOP=true NEXT_PUBLIC_USE_STATIC_EXPORT=true TAURI_BUILD=true npm run build && npm run tauri:fix-paths && node scripts/restore-desktop-build.js && tauri build --target x86_64-pc-windows-gnu && npm run tauri:restore",
    "desktop:build:notarized": "bash scripts/build-notarized.sh",
    "desktop:build:demo:notarized": "bash scripts/build-notarized.sh demo",
    "apple:setup": "bash scripts/setup-apple-auth.sh",
    "apple:sign": "bash scripts/verify-apple-setup.sh",
    "apple:verify": "bash scripts/verify-apple-setup.sh",
    "apple:verify-app": "bash scripts/verify-app-signature.sh",
    "apple:notarize": "bash scripts/notarize-app.sh",
    "tauri:dev": "npm run desktop:dev",
    "tauri:build": "node scripts/tauri-build.js",
    "tauri": "tauri build",
    "tauri:prep": "node scripts/prepare-tauri.js",
    "tauri:fix-paths": "node scripts/fix-tauri-paths.js",
    "tauri:restore": "node scripts/restore-tauri.js",
    "postdesktop:build": "node scripts/post-desktop-build.js",
    "postdesktop:build:demo": "node scripts/post-desktop-build.js demo",
    "clean": "rm -rf .next out dist node_modules/.cache",
    "clean:full": "npm run clean && rm -rf node_modules && npm install",
    "clean:tauri": "rm -rf src-desktop/target",
    "test:coresignal": "jest --config tests/coresignal/jest.config.js",
    "test:voice": "playwright test tests/e2e/voice-integration.test.js",
    "test:voice:ui": "playwright test tests/e2e/voice-integration.test.js --ui",
    "test:voice:debug": "playwright test tests/e2e/voice-integration.test.js --debug",
    "test:voice:all": "./scripts/test/run-voice-tests.sh",
    "test:all": "npm run test:coresignal && npm run test:integration && npm run test:e2e",
    "test:production": "npm run test:coresignal && npm run test:integration",
    "test:ci": "npm run test:production && npm run test:e2e --reporter=github",
    "validate:production": "npm run type-check && npm run lint && npm run test:production",
    "db:generate": "prisma generate --schema=prisma/schema-streamlined.prisma",
    "db:generate:safe": "node scripts/fix-prisma-lock.js",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "analyze": "cross-env ANALYZE=true npm run build",
    "lighthouse": "lhci autorun",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "deps:update": "npx npm-check-updates -u",
    "deps:check": "npx npm-check-updates",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev:mobile": "cd mobile && expo start",
    "build:mobile:ios": "cd mobile && eas build --platform ios",
    "build:mobile:android": "cd mobile && eas build --platform android",
    "test:mobile": "cd mobile && npm test",
    "test:mobile:watch": "cd mobile && npm run test:watch",
    "test:mobile:coverage": "cd mobile && npm run test:coverage",
    "lint:mobile": "cd mobile && npm run lint",
    "lint:mobile:fix": "cd mobile && npm run lint:fix"
  },
