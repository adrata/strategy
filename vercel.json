{
  "regions": ["iad1", "sfo1", "dub1"],
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/api/top100/**/*.ts": {
      "maxDuration": 300,
      "memory": 3008
    }
  },
  "crons": [
    {
      "path": "/api/top100/warmup",
      "schedule": "*/5 * * * *"
    }
  ],
  "headers": [
    {
      "source": "/adrata_buyer_group_intelligence_annoucement_optimized.mp4",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/private/sbi-growth/flexera-bgi-case",
      "destination": "/private/sbi-growth/flexera-bgi-case"
    },
    {
      "source": "/private/cloudcaddie/tim-frost-presales-report",
      "destination": "/private/cloudcaddie/tim-frost-presales-report"
    },
    {
      "source": "/private/(.*)",
      "destination": "/private/$1"
    }
  ]
} 