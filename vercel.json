{
  "crons": [
    {
      "path": "/api/v1/chronicle/scheduler",
      "schedule": "0 9 * * 5"
    },
    {
      "path": "/api/cron/email-sync",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/check-reminders",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/data-refresh",
      "schedule": "0 2 * * *",
      "description": "Daily data refresh for high-risk contacts"
    },
    {
      "path": "/api/cron/enrich-all-workspaces",
      "schedule": "0 2 * * 0",
      "description": "Weekly enrichment for all workspace customers"
    }
  ],
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/api/v1/communications/email/sync/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/webhooks/nango/email/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/v1/chronicle/scheduler/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/email-sync/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/check-reminders/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/data-refresh/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/enrich-all-workspaces/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/webhooks/coresignal-realtime/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/ai/notifications/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/v1/$1"
    }
  ],
  "redirects": [
    {
      "source": "/notary-everyday",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/notary-everyday/:path*",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/notaryeveryday",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/notaryeveryday/:path*",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/ne",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/ne/:path*",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/adrata",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/adrata/:path*",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/rps",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/rps/:path*",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/top",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/top/:path*",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/demo",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/demo/:path*",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/cloudcaddie",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/cloudcaddie/:path*",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/pinpoint",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/pinpoint/:path*",
      "destination": "/sign-in",
      "permanent": false
    }
  ]
}