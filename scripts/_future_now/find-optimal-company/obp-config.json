  "behavioralPrinciples": {
    "anchoring": {
      "description": "New leaders anchor on their previous company's approach",
      "applicationRule": "If leader came from Tier 1 company, they expect similar tooling/ratios"
    },
    "loss_aversion": {
      "description": "Pain of loss is 2x stronger than pleasure of equivalent gain",
      "applicationRule": "Frame around what they'll lose without action, not just what they'll gain"
    },
    "social_proof": {
      "description": "People follow what similar others do",
      "applicationRule": "Reference what their previous company or competitors do"
    },
    "commitment_consistency": {
      "description": "People want to be consistent with past commitments",
      "applicationRule": "If they've publicly committed to compliance, remind them"
    },
    "scarcity": {
      "description": "Limited availability increases perceived value",
      "applicationRule": "Emphasize closing timeline windows (90-day, board meeting, audit)"
    },
    "authority": {
      "description": "People defer to experts and authority figures",
      "applicationRule": "Champion's previous company experience gives them internal authority"
    }
  },

